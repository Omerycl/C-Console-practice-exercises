using System;
class HelloWorld {
  static void Main() {
   
   
   
int sayi1 = 1;
int sayi2 = 1;

while (sayi1<=10)
{
    while(sayi2<=10)
    {
        Console.WriteLine(sayi1+"*"+sayi2+"="+sayi1*sayi2);
        sayi2++;
    }
    sayi1++;
    sayi2 =1;
}



    

    
    
    
    Console.ReadLine();
    
    
    
    
  }
}
